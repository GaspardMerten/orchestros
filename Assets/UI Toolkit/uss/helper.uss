.row {
    flex-direction: row;
    justify-content: space-between;
    padding: 8px 2px;
    border-bottom-color: rgb(202, 202, 202);
    border-bottom-width: 1px;
    margin-right: 10px;
}

.description {
    color: rgb(82, 82, 82);
    font-size: 16px;
}

.shortcut {
    color: rgb(65, 86, 219);
    font-size: 16px;
    -unity-font-style: bold;
}